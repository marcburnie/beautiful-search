{
  "manifest_version": 2,
  "name": "Search Inspiration",
  "version": "1.0",
  "description": "Brighten your day with scenic photo backgrounds and inspirational quotes for your Google search page.",
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "all_frames": true,
      "js": ["jquery.min.js", "main.js"],
      "css": ["index.css"]
    }
  ],
  "page_action": {
    "default_icon": "img.png",
    "default_title": "Have a beautiful day"
  }
}
